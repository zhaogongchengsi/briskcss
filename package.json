{
  "name": "brisk-css",
  "version": "0.0.4",
  "description": "Lightning CSS CLI",
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "brisk": "./dist/cli.mjs"
  },
  "packageManager": "pnpm@7.12.0",
  "keywords": [],
  "author": "zhaogongchengsi <zzhhaon@163.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/zhaogongchengsi",
  "homepage": "https://github.com/zhaogongchengsi/briskcss#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhaogongchengsi/briskcss.git"
  },
  "bugs": "https://github.com/zhaogongchengsi/briskcss/issues",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.11.19",
    "bumpp": "^8.2.1",
    "esno": "^0.16.3",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.1",
    "vitest": "^0.28.4"
  },
  "dependencies": {
    "c12": "^1.4.2",
    "chokidar": "^3.5.3",
    "citty": "^0.1.2",
    "consola": "^3.2.3",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.3",
    "lightningcss": "^1.21.5",
    "pathe": "^1.1.1"
  }
}
